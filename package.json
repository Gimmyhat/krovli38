{
  "name": "krovli38",
  "version": "1.0.0",
  "description": "Сайт компании по ремонту кровли с административной панелью",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "dev:admin": "cd admin-panel && npm run dev",
    "dev:server": "cd server && npm run dev",
    "start-all": "concurrently \"npm run dev\" \"npm run dev:admin\" \"npm run dev:server\"",
    "build:all": "npm run build && cd admin-panel && npm run build && cd ../server && npm run build",
    "install:all": "npm install && cd admin-panel && npm install && cd ../server && npm install"
  },
  "keywords": [
    "react",
    "vite",
    "typescript",
    "tailwindcss",
    "express",
    "mongodb"
  ],
  "author": "Krovli38",
  "license": "MIT",
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.14.2"
  },
  "devDependencies": {
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "autoprefixer": "^10.4.14",
    "concurrently": "^8.2.0",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "postcss": "^8.4.27",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.0.2",
    "vite": "^4.4.5"
  }
}
