{
  "name": "krovli38",
  "private": true,
  "version": "1.0.0",
  "description": "Сайт компании по ремонту кровли с административной панелью",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "dev:admin": "cd admin-panel && npm run dev",
    "dev:server": "cd server && npm run dev",
    "start-all": "concurrently \"npm run dev\" \"npm run dev:admin\" \"npm run dev:server\"",
    "build:all": "npm run build && cd admin-panel && npm run build && cd ../server && npm run build",
    "install:all": "npm install && cd admin-panel && npm install && cd ../server && npm install",
    "docker:local": "docker compose -f docker/docker-compose.yml -f docker/docker-compose.local.yml --env-file docker/.env.local up -d --build",
    "docker:local:down": "docker compose -f docker/docker-compose.yml -f docker/docker-compose.local.yml --env-file docker/.env.local down",
    "docker:prod": "docker compose -f docker/docker-compose.yml --env-file docker/.env.prod up -d --build",
    "docker:prod:down": "docker compose -f docker/docker-compose.yml --env-file docker/.env.prod down",
    "docker:build": "docker compose -f docker/docker-compose.yml build",
    "docker:push": "docker compose -f docker/docker-compose.yml push",
    "scan-images": "node scripts/image-scanner.js",
    "scan-images:docker": "docker exec -it docker-server-1 node scripts/image-scanner.js"
  },
  "keywords": [
    "react",
    "vite",
    "typescript",
    "tailwindcss",
    "express",
    "mongodb"
  ],
  "author": "Krovli38",
  "license": "MIT",
  "dependencies": {
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@hookform/resolvers": "^4.1.2",
    "@mui/icons-material": "^5.15.10",
    "@mui/material": "^5.15.10",
    "@reduxjs/toolkit": "^2.2.1",
    "axios": "^1.6.7",
    "lucide-react": "^0.476.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.54.2",
    "react-input-mask": "^2.0.4",
    "react-redux": "^9.1.0",
    "react-router-dom": "^6.22.1",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@types/node": "^20.11.19",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@types/react-input-mask": "^3.0.6",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.14",
    "concurrently": "^8.2.0",
    "dotenv": "^16.4.7",
    "eslint": "^8.56.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "form-data": "^4.0.2",
    "glob": "^11.0.1",
    "postcss": "^8.4.27",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.2.2",
    "vite": "^4.5.0"
  }
}
